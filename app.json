{
  "expo": {
    "name": "IC Finder",
    "slug": "ic-finder",
    "scheme": "icfinder",
    "version": "0.1.0",
    "icon": "./assets/images/icon.png",
    "orientation": "portrait",
    "newArchEnabled": true,
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "CAMERA"
      ],
      "package": "com.ubed.icfinder",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera"
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "56acfb2c-a92f-4895-8ea9-a7ba6bd69b0d"
      }
    }
  }
}
