{
  "expo": {
    "name": "IC Finder",
    "slug": "ic-finder",
    "scheme": "icfinder",
    "version": "0.1.0",
    "icon": "./assets/images/icon.png",
    "orientation": "portrait",
    "newArchEnabled": true,
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": ["CAMERA"]
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      ["expo-camera"]
    ],
    "extra": {
      "eas": {
        "projectId": "00000000-0000-0000-0000-000000000000"
      }
    }
  }
}
